terraform {
  required_providers {
    azurerm = {
      source  = "hashicorp/azurerm"
      version = "=2.98.0"
     }
  }
   backend "azurerm" {
    resource_group_name  = "rg-permanent"
    storage_account_name = "mrwterraformstate"
    container_name       = "terraform-state"
    key                  = "terraform-state-azure-orbital"
  }
}

provider "azurerm" {
  features {}
}